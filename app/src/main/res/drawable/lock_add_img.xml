<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:color="@color/themeColor" android:width="1dp" android:dashGap="4dp" android:dashWidth="4dp"/>
        </shape>

    </item>
    <item android:width="40dp" android:height="40dp" android:gravity="center">
        <selector>
            <item android:drawable="@drawable/music_add_dime" android:state_pressed="false"/>
            <item android:state_pressed="true" android:drawable="@drawable/music_add_light"/>
        </selector>
    </item>
</layer-list>